mixin premio(p, i)
	-let g = p.grupo
	.PREMIO(class=i%2==0?"PREMIO--STRIPED":"")
		.PREMIO__MILHAR
			span.MILHAR__POS= (i+1)+"ยบ"
			.MILHAR__NUM= p.milhar
		.PREMIO__GRUPO
			.GRUPO__NUM= g.num
			img.GRUPO__IMG(src="/images/grupos/"+g.img128, alt=g.nome, srcset=`/images/grupos/${g.img}, /images/grupos/${g.img128} 1.5x` data-toggle="tooltip" data-placement="top" title=g.nome)